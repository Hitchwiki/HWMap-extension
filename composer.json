{
  "name": "HWMap",
  "type": "mediawiki-extension",
  "description": "Hitchwiki.org Map extension for MediaWiki. Requires Semantic MediaWiki.",
  "keywords": [
    "Hitchwiki",
    "Hitchhiking",
    "Map",
    "MediaWiki",
    "SMW",
    "Semantic MediaWiki"
  ],
  "homepage": "https://github.com/Hitchwiki/HWMap-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Hitchwiki",
      "homepage": "http://hitchwiki.org"
    }
  ],
  "support": {
    "irc": "irc://irc.freenode.net/hitchhiking",
    "issues": "https://github.com/Hitchwiki/HWMap-extension/issues",
    "source": "https://github.com/Hitchwiki/HWMap-extension"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "sphericalgeometry",
        "version": "@dev",
        "type": "mediawiki-extension",
        "dist": {
          "url": "https://github.com/tubalmartin/spherical-geometry-php/archive/1.1.1.zip",
          "type": "zip"
        }
      }
    }
  ],
  "require": {
		"mediawiki/semantic-media-wiki": "~2.0",
    "sphericalgeometry": "@dev"
  },
  "scripts": {
    "post-update-cmd": "bower install --config.interactive=false --allow-root",
    "post-install-cmd": "bower install --config.interactive=false --allow-root"
  },
  "config": {
    "vendor-dir": "lib/"
  }
}
